{
  "manifest_version": 3,
  "name": "Whitelist",
  "version": "1.0",
  "description": "Cater your YouTube experience to what you need.",
  "permissions": [
    "activeTab",
    "storage"
  ],
	"background" : {
		"service_worker": "./src/background.js",
		"persistent": false
	},
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
			"css": ["./src/styles/ytstyle.css"],
      "js": ["./src/index.js"]
    }
  ]
}
