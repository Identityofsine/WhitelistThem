{
  "manifest_version": 2,
  "name": "Whitelist",
  "version": "1.0",
  "description": "Cater your YouTube experience to what you need.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    //"default_icon": "icon.png",
    //"default_popup": "popup.html",
    //"default_title": "YouTube Modifier"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["./src/index.js"]
    }
  ]
}
