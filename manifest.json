{
  "manifest_version": 3,
  "name": "Whitelist",
  "version": "1.0",
  "description": "Cater your YouTube experience to what you need.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["./src/index.js"]
    }
  ]
}
